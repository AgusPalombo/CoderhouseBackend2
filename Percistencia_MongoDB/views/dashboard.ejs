<!DOCTYPE html>
<html>
<head>
    <title>Dashboard</title>
</head>
<body>
    <h1>Bienvenido <%= user.username %></h1>
    
    <h2>Historial de inicios de sesión:</h2>
    <ul>
    <% loginHistory.forEach(function(login) { %>
        <li>
            <%= login.timestamp.toLocaleString() %> - 
            <%= login.successful ? 'Exitoso' : 'Fallido' %>
        </li>
    <% }); %>
    </ul>

    <a href="/auth/logout">Cerrar sesión</a>
</body>
</html>